    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "lint-staged": "^12.1.2",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@types/module-alias": "^2.0.1",
    "module-alias": "^2.2.2",
